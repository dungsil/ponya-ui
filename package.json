{
  "private": true,
  "name": "ponya",
  "version": "1.0.0-next.3",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "type": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.css"
  ],
  "scripts": {
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lintfix": "pnpm lint --fix",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "unbuild",
    "deploy:ci": "bumpp --preid next"
  },
  "dependencies": {
    "@unocss/core": "^0.50.6"
  },
  "devDependencies": {
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "typescript": "^5.0.2",
    "unbuild": "^1.1.2",
    "unocss": "^0.50.6",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  }
}
